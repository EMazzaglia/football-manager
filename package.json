{
  "name": "football-manager",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --testMatch='**/test/unit/**/*.spec.ts'",
    "test:e2e": "jest --testMatch='**/test/e2e/**/*.spec.ts'",
    "start": "node -r dist/main.js",
    "seed": "npx ts-node migrations/seed.ts migrations/events.csv",
    "build": "tsc && cp tsconfig.json dist/",
    "docker:seed": "docker-compose exec app npx ts-node migrations/seed.ts migrations/events.csv",
    "docker:build": "docker-compose up --build",
    "docker:start": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:dev": "nodemon --legacy-watch --watch 'src/**/*.ts' src/main.ts",
    "docker:test:email": "docker-compose exec app npx ts-node test/notifications.ts both",
    "docker:test:email:direct": "docker-compose exec app npx ts-node test/notifications.ts direct",
    "docker:test:email:scheduler": "docker-compose exec app npx ts-node test/notifications.ts scheduler"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^4.3.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mongoose": "^8.14.1",
    "mongoose-paginate-v2": "^1.9.0",
    "nodemailer": "^7.0.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/cron": "^2.0.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/mongoose": "^5.11.96",
    "@types/mongoose-paginate-v2": "^1.6.4",
    "@types/node": "^22.15.3",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
